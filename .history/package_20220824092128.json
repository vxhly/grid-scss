    "grid",
    "scss",
    "sass",
    "grid-sass",
    "sass-grid",
    "Grid",
    "Layout",
    "grid.css",
    "Grid"
  ],
  "author": "vxhly <pengchengou@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vxhly/grid-scss/issues"
  },
  "homepage": "https://github.com/vxhly/grid-scss#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
